{
  "name": "s2f",
  "version": "0.1.0",
  "description": "Step based workflow engine (Start to finish)",
  "license": "MIT",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bhoriuchi/s2f.git"
  },
  "scripts": {
    "build:copybrc": "cp build/.build.babelrc src/.babelrc",
    "build:rmbrc": "rm -f src/.babelrc",
    "buildexample": "npm run build && rollup example/example.js --output example/build.js --format cjs && node example/build.js --harmony",
    "example": "node example/index.js --harmony",
    "build": "npm run build:copybrc && rollup -c build/rollup.config.js && npm run build:rmbrc",
    "test": "npm run build && mocha test/index.js",
    "test:unit": "npm run build && mocha test/index.js -t 'unit'",
    "test:compile": "npm run build && mocha test/index.js -t 'unit.compile'",
    "test:utils": "npm run build && mocha test/index.js -t 'unit.utils'",
    "test:types": "npm run build && mocha test/index.js -t 'unit.types'",
    "test:query": "npm run build && mocha test/index.js -t 'unit.query'",
    "copytemporal": "cp ../graphql-factory-temporal/*.js node_modules/graphql-factory-temporal/",
    "copyyj": "cp ../yellowjacket/*.js node_modules/yellowjacket/"
  },
  "author": "Branden Horiuchi <bhoriuchi@gmail.com>",
  "dependencies": {
    "bunyan": "^1.8.1",
    "chalk": "^1.1.3",
    "graphql-factory": "^0.4.6",
    "graphql-factory-temporal": "git+https://github.com/bhoriuchi/graphql-factory-temporal.git#master",
    "graphql-factory-types": "^0.1.0",
    "lodash": "^4.13.1",
    "node-getopt": "^0.2.3",
    "sbx": "^2.0.0",
    "socket.io": "^1.4.0",
    "socket.io-client": "^1.4.8",
    "yellowjacket": "git+https://github.com/bhoriuchi/yellowjacket.git#master"
  },
  "devDependencies": {
    "chai": "^3.5.0",
    "babel-core": "^6.0.0",
    "babel-preset-es2015": "6.13.0",
    "babel-preset-stage-2": "^6.0.0",
    "babel-plugin-transform-runtime": "^6.0.0",
    "babel-runtime": "^6.9.0",
    "babel-register": "^6.11.0",
    "graphql": "^0.6.0",
    "mocha": "^3.0.0",
    "rethinkdbdash": "^2.3.6",
    "rewire": "^2.5.1",
    "rollup": "^0.34.0",
    "rollup-plugin-babel": "^2.6.0",
    "babel-preset-es2015-rollup": "^1.1.0"
  },
  "bugs": {
    "url": "https://github.com/bhoriuchi/s2f/issues"
  },
  "homepage": "https://github.com/bhoriuchi/s2f#readme",
  "directories": {
    "example": "example"
  },
  "keywords": [
    "orchestration",
    "workflow",
    "vm",
    "eval",
    "step",
    "sandbox"
  ]
}
